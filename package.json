    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@capacitor-community/media": "^5.4.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/filesystem": "^5.2.0",
    "@capacitor/toast": "^5.0.6",
    "@cds/ng-core": "^2.3.7",
