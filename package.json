    "@cds/ng-themes": "^2.3.7",
    "@cds/ng-web-components": "^2.3.7",
    "@cds/style-dictionary": "^2.3.7",
    "@ionic/angular": "^7.6.1",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/store": "^17.0.1",
    "@ngrx/store-devtools": "^17.0.1",
    "ngx-skeleton-loader": "^8.0.1",
    "rxjs": "~7.8.0",
